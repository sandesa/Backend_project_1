{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "logs/app.log",
      "IncludeScopes": false,
      "FileSizeLimit": 10240, // 10 MB
      "MaxRollingFiles": 7
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=WebbshopDb;Integrated Security=True;MultipleActiveResultSets=true"
  }
}
